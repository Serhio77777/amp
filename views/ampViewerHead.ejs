        @media screen and (max-width: 1023px) {
            .single-text {
                font-size: 2.6vw;
            }
            .animate {
                font-size: 3.434vw;
            }
            .rotate-wrapper {
                font-size: 3.434vw;
            }
            @media screen and (max-aspect-ratio: 9/16) {
                .animate {
                    font-size: 1.945vh;
                }
                .single-text {
                    font-size: 1.45vh;
                }
            }
        }
        @media (max-width: 1023px) {
            @media screen and (min-aspect-ratio: 9/12) {
                .single-text {
                    font-size: 1.55vh ;
                }
                .animate {
                    font-size: 2vh;
                }
                amp-story-grid-layer {
                    height: 100vh;
                    margin: auto;
                    width: 56.25vh;
                }
                .rotate-wrapper {
                    font-size: 2vh;
                }
            }

            @media screen and (max-aspect-ratio: 9/20) {
                .single-text {
                    font-size: 2.4vw;
                }
                div.animate {
                    font-size: 3.2vw;
                }
                amp-story-grid-layer {
                    height: 156.25vw;
                    margin: auto;
                    width: 100vw;
                }
            }
        }
        @media screen and (min-width: 1024px) {
            .single-text {
                font-size: 1.53vh;
            }
            div.animate {
                font-size: 2vh;
            }
        }
