<div class="amp-shape"
     style="width: 100%;height: 100%;opacity: <%=layer.settings.generalSettings.opacity / 100 %>;">
  <div class="inner-item shape" style="width: 100%;height: 100%;">
    <div
            id="shape<%= layer.settings.generalSettings.position ? layer.settings.generalSettings.position : '' %>"
            style="width: 100%;height: 100%;">
      <amp-img
              width="<%= layer.settings.layerSettings.width %>px"
              height="<%= layer.settings.layerSettings.height %>px"
              layout="responsive"
              alt="an image"
              style="width:100%;height:100%;object-fit:fill;position: static;opacity: <%=layer.settings.generalSettings.opacity / 100 %>;"
              src="<%- include('shapes/' + layer.content.value, {layer: layer}) %>"
      ></amp-img>
    </div>
  </div>
</div>
