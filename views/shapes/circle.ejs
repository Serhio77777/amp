<svg
        xmlns='http://www.w3.org/2000/svg'
        xmlns:xlink='http://www.w3.org/1999/xlink'
        width='<%= layer.settings.layerSettings.width ? layer.settings.layerSettings.width : 100 %>'
        height='<%= layer.settings.layerSettings.height ? layer.settings.layerSettings.height : 100 %>'
        style='overflow: visible;filter: filter:drop-shadow(0px 0px <%= layer.settings.generalSettings.shadow / 10 %>px rgba(0,0,0,0.9));'
        viewBox='0 0 33 33'
        preserveAspectRatio='none'
>
    <g>
        <g transform='translate(-1059 -228.25)'>
            <use fill='transparent' />
            <path
                    d='M1059 244.75a16.5 16.5 0 1 1 33 0 16.5 16.5 0 0 1-33 0z'
                    stroke='<%= layer.settings.layerSettings.shapeStyles.borderColor ? layer.settings.layerSettings.shapeStyles.borderColor : 'black' %>'
                    fill='<%= layer.settings.layerSettings.shapeStyles.fillColor ? layer.settings.layerSettings.shapeStyles.fillColor : 'transparent' %>'
                    stroke-width='<%= layer.settings.layerSettings.shapeStyles.thickness && layer.settings.layerSettings.shapeStyles.thickness >= 0 ? layer.settings.layerSettings.shapeStyles.thickness + 'px' : 0 %>'
                    vector-effect='non-scaling-stroke'>
            </path>
        </g>
    </g>
</svg>
